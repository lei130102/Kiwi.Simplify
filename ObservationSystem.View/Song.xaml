<Window x:Class="ObservationSystem.View.Song"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ObservationSystem.View"
        mc:Ignorable="d"
        Title="Song" Height="450" Width="800"
        DataContext="{Binding Song, Source={StaticResource Locator}}">
    <StackPanel>
        <TextBlock Text="{Binding Path=Title}"/>
        <TextBlock Text="{Binding Path=Artist}"/>
    </StackPanel>
</Window>
